# GovChat Configuration Example
# Copy this to .env and customize for your setup

# Target chat website URL
CHAT_WEBSITE_URL=https://govchat.ars.usda.gov

# API endpoint (leave empty for auto-discovery)
CHAT_API_ENDPOINT=

# Use Selenium browser automation instead of HTTP
USE_SELENIUM=false

# Server configuration
HOST=127.0.0.1
PORT=8000

# Timeout for chat responses (seconds)
CHAT_TIMEOUT=120

# Selenium options
SELENIUM_HEADLESS=false
